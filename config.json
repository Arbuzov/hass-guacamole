{
    "name": "Guacamole",
    "version": "1.3.0",
    "slug": "haguacamole",
    "description": "Apache Guacamole is a clientless remote desktop gateway. It supports standard protocols like VNC, RDP, and SSH.",
    "arch": ["amd64","armhf"],
    "url": "https://guacamole.apache.org/",
    "startup": "application",
    "boot": "auto",
    "panel_icon": "mdi:remote-desktop",
    "ingress": true,
    "ingress_port": 8080,
    "image": "bleakvin/hass-guacamole",
    "options": {},
    "ports": {
    "8080/tcp": null
    },
    "ports_description": {
      "8080/tcp": "Web interface (Not required for Home Assistant Ingress)"
    },
    "schema": {
      "certfile": "str?",
      "keyfile": "str?",
      "ssl": "bool?"
    },
    "hassio_role": "default"
}
